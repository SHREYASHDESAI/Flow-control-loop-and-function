import java.util.Scanner;

public class FirstNum {
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);

        System.out.println("Enter a number:");
        int num = s.nextInt();

        while(num>=10)
        {
            num = num/10;
        }
        System.out.println("First digit is:" + num);
    }
}
