import java.util.Scanner;

public class Table {
    public static void main(String[] args){
        Scanner s = new Scanner(System.in);

        System.out.println("Enter Number whose table you want:");
        int n = s.nextInt();
        int i;

        for(i=1;i<=10;i++)
        {
            int a;
            a = n*i;
            System.out.println(a);

        }
    }
}
